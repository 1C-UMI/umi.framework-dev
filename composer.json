{
    "name": "umi/framework-dev",
    "description": "UMI.Framework Development Package",
    "keywords": ["umi", "framework", "development"],
    "type": "library",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "homepage": "http://umi-framework.ru/",
    "replace": {
        "umi/framework" : "self.version"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "umi/extension-twig": "*"
    },
    "suggest": {
        "umi/extension-twig": "For using Twig template engine."
    },
    "autoload": {
        "psr-4": {
            "umi\\": "library/",
            "umi\\extension\\twig\\" : "extension/twig"
        }
    }
}