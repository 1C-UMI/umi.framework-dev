    "require": {
        "php": ">=5.4.0",
        "lib-pcre": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",

        "umisoft/phpmorphy": "1.*",
        "symfony/http-foundation": "2.4.*",
        "psr/log": "1.*",
        "doctrine/dbal": "2.4.*",
