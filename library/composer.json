{
    "name": "umisoft/umi.framework",
    "description": "UMI.Framework Version 1",
    "keywords": ["umi", "framework"],
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "http://umi-framework.ru/",

    "require": {
        "php": ">=5.4.0",
        "lib-pcre": "*",
        "ext-mbstring": "*",

        "makhov/phpmorphy": "dev-master",
        "symfony/http-foundation": "2.4.*",
        "psr/log": "1.*",
        "doctrine/dbal": "2.4.*",
        "twig/twig": "1.*"
    },

    "suggest": {
        "umi/extension-twig": "templating with Twig extension.",
        "umi/extension-monolog": "logging with Monolog extension."
    },

    "autoload": {
        "psr-4": {
            "umi\\": ""
        }
    }
}